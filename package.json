{
  "name": "preact-observables",
  "sideEffects": false,
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melnikov-s/preact-observables.git"
  },
  "author": "Sergey Melnikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/melnikov-s/preact-observables/issues"
  },
  "homepage": "https://github.com/melnikov-s/preact-observables#readme",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/preact-observables.es.js",
  "main": "./dist/preact-observables.umd.js",
  "exports": {
    ".": {
      "import": "./dist/preact-observables.es.js",
      "require": "./dist/preact-observables.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "yarn build",
    "perf": "vitest -c vite.perf.config.js"
  },
  "peerDependencies": {
    "@preact/signals-core": "*"
  },
  "devDependencies": {
    "@preact/signals": "^1.1.1",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^20.0.1",
    "preact": "^10.11.1",
    "prettier": "^2.7.1",
    "sinon": "^14.0.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.23.2"
  },
  "dependencies": {
    "nu-observables": "^0.0.5"
  }
