    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "vitest run current file",
            "program": "${workspaceFolder}/node_modules/.bin/vitest",
            "args": [
              "${fileBasename}",
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
          }
    ]
}